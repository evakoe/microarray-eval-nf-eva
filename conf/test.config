/*
========================================================================================
    Nextflow config file for running minimal tests
========================================================================================
    Defines input files and everything required to run a fast and simple pipeline test.
    Use as follows:
        nextflow run genepi/exome-cnv-nf -r <tag> -profile test,<docker/singularity>
----------------------------------------------------------------------------------------
*/

params {
    project                        = "hg19"
    strand_data                    = "test-data/strand_data/hg19/*strand"
    sequence_data                  = "test-data/sequence_data/*vcf.gz"
    imputation_host                = "https://imputationserver.sph.umich.edu/"
    imputation_panel               = "hapmap-2"
    imputation_build               = "hg19"
    imputation_population          = "eur"
    sequence_build                 = "hg19"
    imputation_token = "XXXX"

}
